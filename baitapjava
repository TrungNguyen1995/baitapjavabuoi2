Scanner scanner  = new Scanner(System.in);

        System.out.println("Mời bạn nhập tên vào:");
        String name10 = scanner.nextLine();
        System.out.println(name10);

        System.out.println("Mời bạn nhập tuổi vào:");
        int age = scanner.nextInt();
        System.out.println(age);
        String one =scanner.nextLine();
        System.out.println("Mời bạn nhập địa chỉ:");
        String address10 = scanner.nextLine();
        System.out.println(address10);

        System.out.println("Mời bạn nhập email:");
        String Email = scanner.nextLine();
        System.out.println(Email);


        System.out.println("Mời bạn nhập tên hàng nhập kho:");
        String tenHang = scanner.nextLine();
        System.out.println(tenHang);

        System.out.println("Mời bạn nhập ngày tháng năm sinh:");
        String birthDate = scanner.nextLine();
        LocalDate localDate10 = LocalDate.parse(birthDate, DateTimeFormatter.ofPattern("yyyy/MM/dd"));
        System.out.println(localDate10);

        System.out.println("Mời bạn nhập thời gian nhập hàng:");
        String timeNhaphang = scanner.nextLine();
        LocalDateTime  deliveryTime =LocalDateTime.parse(timeNhaphang,DateTimeFormatter.ofPattern("yyyy/MM/dd HH:mm:ss"));
        System.out.println(deliveryTime);

        System.out.println("Mời bạn nhập thời gian:");
        String thoiGian = scanner.nextLine();
        LocalTime timeGiaohang =LocalTime.parse(thoiGian, DateTimeFormatter.ofPattern("HH:mm:ss"));
        System.out.println(timeGiaohang);
